---
import MainFooter from "../components/footers/main-footer.astro";
import MainHeader from "../components/headers/main-header.astro";
import Layout from "../layouts/layout.astro";
---

<Layout title="Smart Bird Home Automations | Oxford & Bucks">
  <div class="boxed_wrapper gradient-color">
    <MainHeader />
    <section
      class="page-title error-page"
      style="background-image: url(assets/images/background/page-title.jpg);"
    >
      <div class="auto-container">
        <div class="content-box">
          <div class="title">
            <!-- <h1>Error Page</h1> -->
          </div>
        </div>
      </div>
    </section>
    <section class="error-section centred">
      <div class="auto-container">
        <div class="error-content">
          <div class="inner-box">
            <figure class="image-box">
              <img src="assets/images/icons/error.png" alt="" />
            </figure>
            <h2>Oops! Page Not Found</h2>
            <p>
              Sorry weâ€™re unable to find the page you are looking for, please
              check <br />the url and try again.
            </p>
            <a href="/" class="theme-btn style-one">Back to Home</a>
          </div>
        </div>
      </div>
    </section>
    <MainFooter />
  </div>
</Layout>
