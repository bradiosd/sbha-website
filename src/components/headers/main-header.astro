  @media only screen and (max-width: 767px) {
    .banner-section.style-four .banner-carousel .slide-item {
      padding-top: 200px;
    }
  }

  @media only screen and (max-width: 499px) {
    .main-header.style-four .header-top .social-links {
      margin-top: 5px;
      float: inherit;
    }
  }

  @media only screen and (max-width: 1200px) {
    .page-header-mobile-info {
      display: none !important;
    }

    .auto-container.xyz {
      padding: 0px;
    }

    .auto-container.xyz .main-box {
      border-radius: 0px !important;
    }

    .auto-container.xyz .outer-box {
      padding-right: 10px !important;
    }

    .auto-container.xyz .btn-box {
      margin-top: 67px;
      margin-right: 5px;
    }

    .auto-container.xyz .btn-box a {
      border-bottom-left-radius: 5px;
    }

    .auto-container.xyz .logo-mobile {
      display: block;
      margin-top: 5px;
    }

    .auto-container.xyz .mobile-menu .close-btn {
    }
  }

  .main-header.style-four .header-top .social-links {
    margin-right: 10px;
    padding-right: 0;
  }

  .main-header.style-four .header-top .social-links:before {
